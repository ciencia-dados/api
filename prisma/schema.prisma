generator client {
  provider = "prisma-client-js"
  //output   = "../generated/prisma"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model data {
  id        Int      @id @default(autoincrement())
  espId     String   @unique
  valor     String
  timestamp DateTime @default(now())

  @@map("data")
}
